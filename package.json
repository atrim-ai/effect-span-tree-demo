{
  "name": "effect-span-tree-demo",
  "version": "1.0.0",
  "description": "Effect-TS SpanTree demo - visualize deeply nested traces with automatic path tracking",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "npx @effect/language-service patch",
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "effect",
    "effect-ts",
    "opentelemetry",
    "tracing",
    "span-tree",
    "observability"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.23.0",
  "dependencies": {
    "@effect/opentelemetry": "npm:@clayroach/effect-opentelemetry@latest",
    "@effect/platform": "^0.94.1",
    "@effect/platform-node": "^0.104.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.208.0",
    "@opentelemetry/sdk-trace-base": "^2.2.0",
    "@opentelemetry/sdk-trace-node": "^2.2.0",
    "effect": "^3.19.14"
  },
  "devDependencies": {
    "@effect/language-service": "^0.64.1",
    "@types/node": "^25.0.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
